<form class="form-horizontal" role="form" action="/map/new?_csrf={{_csrfToken}}" method="POST" enctype="multipart/form-data">
    <!--<input type="hidden" name="_csrf" value="{{_csrfToken}}" />-->
    <div class="form-group">
        <label for="name" class="col-sm-2 control-label">
            City name
        </label>
        <div class="col-sm-4">
            <input type="text" class="form-control" id="name" name="name" value="{{name}}" />
        </div>
    </div>
    <div class="form-group">
        <label for="osm" class="col-sm-2 control-label">
            OSM File
        </label>
        <div class="col-sm-4">
            <input type="file" class="form-control" id="osm" name="osm" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-4">
            <button type="submit" class="btn btn-default">Save</button>
        </div>
    </div>
</form>